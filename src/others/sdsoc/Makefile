all:
	sdspfm -force -vendor 'Digilent' -name 'zybo_z7_20' -version '1.0' -desc 'Zybo Z7-20 Development board with support for HDMI input and HDMI output' -xpr "./proj_copy/zybo_z7_20/zybo_z7_20.xpr" -bd zybo_z7_20.bd -pfmtcl "./hw_pfm_gen.tcl" -samples "./samples/" -o "./output/" -prebuilt "./prebuilt/" -sds-cfg -arch cortex-a9 -os linux -name "Linux" -id linux -rt cpp -bif "./linux/linux.bif" -readme "./boot_linux/generic.readme" -boot "./boot_linux/" -inc "./include_linux/xfopencv/" -image "./linux/image/" -sds-end -sds-cfg -arch cortex-a9 -os standalone -name "Standalone" -id standalone -rt cpp -bif "./standalone/standalone.bif" -readme "./boot_standalone/generic.readme" -boot "./boot_standalone/" -lscript "./standalone/lscript.ld" -sds-end  -sds-cfg -arch cortex-a9 -os freertos -name "FreeRTOS" -id freertos -rt cpp -bif "./freertos/freertos.bif" -readme "./boot_freertos/generic.readme" -boot "./boot_freertos/" -lscript "./freertos/lscript.ld" -sds-end 
